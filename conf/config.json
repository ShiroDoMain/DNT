  "n_layers": 8,
  "n_head": 8,
  "feed_hidden": 2048,
  "pad_idx": 1,
  "factor": 0.9,
  "patience": 10,
  "clip": 1.0,
  "warmup": 100,
  "pad": "[PAD]",
  "sos": "[SOS]",
  "eos": "[EOS]",
  "eps": 1e-9
