{
  "epochs": 10000,
  "source_lang": "en",
  "target_lang":  "de",
  "eval_interval": 10,
  "save_interval": 1000,
  "save_path": "./save",
  "save_best_model": true,
  "dataset": "./data",
  "padding": true,
  "device": "cuda",
  "batch_size": 128,
  "init_lr": 1e-5,
